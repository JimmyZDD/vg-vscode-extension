{
  "name": "vg-vscode-extension",
  "displayName": "Flutter Generator - VG",
  "description": "It supports the getx library and provides effective creation of business modules for Flutter applications.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "flutter",
    "dart",
    "getx",
    "template",
    "generator",
    "state-management"
  ],
  "homepage": "https://github.com/JimmyZDD/vg-vscode-extension/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyZDD/vg-vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/JimmyZDD/vg-vscode-extension/issues"
  },
  "README": "README.md",
  "publisher": "jimmy",
  "icon": "images/icon.png",
  "activationEvents": [
    "workspaceContains:**/pubspec.yaml",
    "onLanguage:json",
    "*"
  ],
  "pricing": "Free",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.assets-generate",
        "title": "☆ VG: Images x1 x2 Generate",
        "category": "VG"
      },
      {
        "command": "extension.new-getx-routers-generate",
        "title": "VG: Routers Generate",
        "category": "VG"
      },
      {
        "command": "extension.new-getx-create-directory",
        "title": "VG: Create Common Directory",
        "category": "VG"
      },
      {
        "command": "extension.new-getx-getbuilder-page",
        "title": "★ VG: GetBuilder Page",
        "category": "VG"
      },
      {
        "command": "extension.new-getx-stateful-getbuilder-page",
        "title": "VG: StatefulWidget + GetBuilder Page",
        "category": "VG"
      },
      {
        "command": "extension.new-getx-page",
        "title": "VG: Full Page",
        "category": "VG"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.assets-generate",
          "group": "blocGroup@1",
          "when": "explorerResourceIsFolder && resourceFilename == '3.0x'"
        },
        {
          "command": "extension.new-getx-routers-generate",
          "group": "blocGroup@2",
          "when": "explorerResourceIsFolder && resourceFilename == 'pages'"
        },
        {
          "command": "extension.new-getx-create-directory",
          "group": "blocGroup@1",
          "when": "explorerResourceIsFolder && resourceFilename == 'lib'"
        },
        {
          "command": "extension.new-getx-getbuilder-page",
          "group": "blocGroup@1",
          "when": "explorerResourceIsFolder && resourcePath =~ /.?pages.?/i"
        },
        {
          "command": "extension.new-getx-stateful-getbuilder-page",
          "group": "blocGroup@1",
          "when": "explorerResourceIsFolder && resourcePath =~ /.?pages.?/i"
        },
        {
          "command": "extension.new-getx-page",
          "group": "blocGroup@1",
          "when": "explorerResourceIsFolder && resourcePath =~ /.?pages.?/i"
        }
      ]
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "jimp": "^0.22.8",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "semver": "^7.5.1"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^3.12.7",
    "@types/lodash": "^4.14.195",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "~16.18.34",
    "@types/semver": "^7.5.0",
    "@types/vscode": "^1.78.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1"
  }
}